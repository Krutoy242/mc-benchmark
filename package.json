{
  "name": "mc-benchmark",
  "type": "module",
  "version": "1.0.9",
  "packageManager": "pnpm@10.10.0",
  "description": "Build charts about load time of Minecraft modpack.",
  "author": "Krutoy242 <krutoy242@gmail.com> (https://github.com/Krutoy242)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Krutoy242/mc-benchmark"
  },
  "keywords": [
    "minecraft",
    "chart",
    "quickchart",
    "benchmark",
    "mods",
    "debug",
    "modpack"
  ],
  "main": "dist/index.js",
  "bin": "dist/cli.js",
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "preview": "vite-node src/cli.ts --cwd=D:/mc/E2E-E -vv --data=src/template.hbs.json > examples/benchmark.md",
    "preview-nospaces": "vite-node scripts/nospaces.ts && vite-node src/cli.ts --cwd=D:/mc/E2E-E -vv --data=src/template-nospaces.hbs.json --template=src/template-nospaces.hbs > examples/benchmark-nospaces.md",
    "build": "vite-node scripts/build.ts && vite build",
    "release": "pnpm build && release-it"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "color": "^5.0.0",
    "color-hash": "^2.0.2",
    "handlebars": "^4.7.8",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@types/color-hash": "^2.0.0",
    "@types/memoizee": "^0.4.12",
    "@types/node": "^22.15.3",
    "@types/numeral": "^2.0.5",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.24.0",
    "eslint-plugin-format": "^1.0.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-node": "^3.1.3",
    "vite-plugin-static-copy": "^2.3.1",
    "vitest": "^3.1.3"
  }
}
